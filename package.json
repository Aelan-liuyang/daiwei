    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^13.4.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-import": "^1.13.8",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.1.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-vue": "^1.1.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.8",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
